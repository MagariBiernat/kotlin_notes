<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kikunote">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.kikunote.activity.DetailNoteActivity"/>
        <activity android:name="com.kikunote.activity.SearchActivity" />
        <activity android:name="com.kikunote.activity.EditActivity" />
        <activity android:name="com.kikunote.activity.MainActivity">
        </activity>
        <activity android:name="com.kikunote.activity.FirstActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.kikunote.activity.LoginActivity" />
        <activity android:name="com.kikunote.activity.RegisterActivity" />
    </application>

</manifest>